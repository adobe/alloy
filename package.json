{
  "name": "@adobe/alloy",
  "version": "2.27.0",
  "description": "Adobe Experience Platform Web SDK",
  "type": "module",
  "main": "dist/alloy.cjs",
  "module": "src/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./src/index.js"
      },
      "require": "./dist/alloy.cjs"
    },
    "./baseCode": {
      "import": {
        "types": "./dist/baseCode.d.ts",
        "default": "./src/baseCode/index.js"
      }
    },
    "./standalone": {
      "import": {
        "types": "./dist/standalone.d.ts",
        "default": "./src/baseCode.js"
      },
      "require": "./dist/alloy.standalone.cjs"
    },
    "./components/EventMerge/createEventMergeId": {
      "import": {
        "types": "./dist/components/EventMerge/createEventMergeId.d.ts",
        "default": "./src/components/EventMerge/createEventMergeId.js"
      }
    },
    "./utils/deepAssign": {
      "import": {
        "types": "./dist/utils/deepAssign.d.ts",
        "default": "./src/utils/deepAssign.js"
      }
    },
    "./core/componentCreators": {
      "import": {
        "types": "./dist/core/componentCreators.d.ts",
        "default": "./src/core/componentCreators.js"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE_BANNER",
    "babel.config.js",
    "rollup.config.js",
    ".browserslistrc",
    "scripts/helpers/entryPointGeneratorBabelPlugin.js",
    "scripts/helpers/path.js"
  ],
  "bin": {
    "alloyBuilder": "scripts/alloyBuilder.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint --cache --fix \"*.{js,cjs,mjs,jsx}\" \"{src,test,scripts,sandbox}/**/*.{js,cjs,mjs,jsx}\"",
    "format": "prettier --write \"*.{html,js,cjs,mjs,jsx}\" \"{sandbox,src,test,scripts}/**/*.{html,js,cjs,mjs,jsx}\"",
    "types": "tsc",
    "test": "npx playwright install chromium && npm run test:build && vitest run && npm run test:scripts",
    "test:coverage": "rimraf coverage && npx playwright install chromium && vitest run --coverage",
    "test:build": "rollup -c --environment SOURCEMAP",
    "pretest:unit": "npm run test:build",
    "test:unit": "npx playwright install chromium && vitest run --project=unit",
    "test:unit:debug": "npm run test:unit:watch -- --no-file-parallelism --browser.headless=false --browser=chromium --browser.provider=playwright",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "pretest:integration": "npm run test:build",
    "test:integration": "npx playwright install chromium && vitest run --project=integration",
    "test:integration:debug": "npm run test:integration --watch --inspect-brk --no-file-parallelism --browser.headless=false --browser=chromium --browser.provider=playwright",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:integration:coverage": "npm run test:integration -- --coverage",
    "pretest:functional": "npm run test:build",
    "test:functional": "EDGE_BASE_PATH=\"ee-pre-prd\" ALLOY_ENV=\"int\" testcafe chrome",
    "test:functional:custom": "node scripts/helpers/runFunctionalTests.js",
    "test:functional:watch": "EDGE_BASE_PATH=\"ee-pre-prd\" ALLOY_ENV=\"int\" ./scripts/watchFunctionalTests.js --browsers chrome",
    "test:functional:debug": "npm run test:functional -- --inspect-brk --debug-mode",
    "test:scripts": "vitest run --config=./scripts/specs/vitest.config.js",
    "dev": "cd sandbox && export REACT_APP_NONCE=321 && npm start",
    "build": "npm run clean && rollup -c --environment BUNDLESIZE && npm run types && echo \"Base Code:\" && cat dist/baseCode.min.js",
    "build:watch": "npm run clean && rollup -c --watch --environment BASE_CODE_MIN,STANDALONE",
    "build:custom": "node scripts/alloyBuilder.js",
    "prepare": "husky && cd sandbox && npm install",
    "prepack": "npm run clean && npm run build && npm run types",
    "checkthattestfilesexist": "./scripts/checkThatTestFilesExist.js",
    "add-license": "./scripts/add-license.js"
  },
  "lint-staged": {
    "./*.{cjs,mjs,js,jsx}": [
      "eslint --cache --fix"
    ],
    "./{src,test,scripts}/**/*.{cjs,mjs,js,jsx}": [
      "eslint --cache --fix"
    ],
    "./*.{html,js,cjs,mjs,jsx}": [
      "prettier --write"
    ],
    "./{sandbox,src,test,scripts}/**/*.{html,js,cjs,mjs,jsx}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/alloy.git"
  },
  "author": "Adobe Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "@adobe/aep-rules-engine": "^3.1.1",
    "@adobe/reactor-cookie": "^1.1.0",
    "@adobe/reactor-load-script": "^1.1.1",
    "@adobe/reactor-object-assign": "^2.0.0",
    "@adobe/reactor-query-string": "^2.0.0",
    "@babel/core": "^7.27.3",
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@inquirer/prompts": "^7.5.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "commander": "^14.0.0",
    "css.escape": "^1.5.1",
    "js-cookie": "3.0.5",
    "rollup": "^4.41.1",
    "rollup-plugin-license": "^3.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/plugin-transform-runtime": "^7.27.3",
    "@eslint/js": "^9.27.0",
    "@octokit/rest": "^22.0.0",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "chalk": "^5.4.1",
    "concurrently": "^9.1.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-testcafe": "^0.2.1",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "handlebars": "^4.7.8",
    "happy-dom": "^17.5.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "msw": "^2.8.5",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "read-cache": "^1.0.0",
    "recursive-readdir": "^2.2.3",
    "rimraf": "^6.0.1",
    "semver": "^7.7.2",
    "staged-git-files": "^1.3.0",
    "testcafe": "^3.7.2",
    "testcafe-browser-provider-saucelabs": "^3.0.0",
    "testcafe-reporter-junit": "^3.0.2",
    "testcafe-reporter-saucelabs": "^3.6.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "url-exists-nodejs": "^0.2.4",
    "url-parse": "^1.5.10",
    "vitest": "^3.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.41.1"
  },
  "overrides": {
    "eslint": "^9.27.0"
  }
}
